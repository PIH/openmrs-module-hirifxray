<?xml version="1.0" encoding="UTF-8"?>
<module configVersion="1.0">
	
	<!-- Base Module Properties -->
	<id>@MODULE_ID@</id>
	<name>@MODULE_NAME@</name>
	<version>@MODULE_VERSION@</version>
	<package>@MODULE_PACKAGE@</package>
	<author>Partners In Health</author>
	<description>HIRIF Study Xray Module</description>
	<activator>@MODULE_PACKAGE@.HirifxrayActivator</activator>
	<require_version>1.9.0</require_version>
	
	<!-- Extensions -->
	
	<extension>
		<point>org.openmrs.headerFullIncludeExt</point>
		<class>@MODULE_PACKAGE@.web.extension.HeaderFullExtension</class>
	</extension>

	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	
</module>